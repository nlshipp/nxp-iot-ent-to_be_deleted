 	default:
@@ -716,6 +718,19 @@ static int frac_pll_init(u32 pll, enum frac_pll_out_val val)
 			FRAC_PLL_REFCLK_DIV_VAL(4) |
 			FRAC_PLL_OUTPUT_DIV_VAL(divq);
 		break;
+	case 600:
+		pll_cfg0 = &ana_pll->vpu_pll_cfg0;
+		pll_cfg1 = &ana_pll->vpu_pll_cfg1;
+
+		if (val == MHZ(600))
